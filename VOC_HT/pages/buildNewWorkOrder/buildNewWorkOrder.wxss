page {
  font-size: 12px;
  width: 100%;
  background: #eee;
}

.activity {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
}

.userinfo {
  display: flex;
  flex-direction: column;
  width: 100%;
  color: #fff;
}

.message {
  padding: 10px 10px 15px 10px;
}

.progress_tx {
  display: flex;
  flex-direction: row;
  width: 100%;
}

.srcoll_tx {
  width: 100%;
  height: 70px;
}

.srcoll_activity {
  width: 100%;
  height: 100%;
}

.tx {
  padding: 3px 0;
}

.progress_tx .tx {
  padding: 3px 10px;
}

.expect-date {
  margin-bottom: 5px;
}

.progress {
  width: 100%;
  padding: 0 0 20px 0;
}

.progress .item {
  position: relative;
  border-left: 2px solid #c8c8c8;
  margin: 0 0 0 20px;
  padding: 10px 0 0 15px;
}

.progress image {
  position: absolute;
  left: -9px;
  top: 30px;
}

.progress image {
  width: 16px;
  height: 16px;
}

.progress-info {
  background: #fff;
  border-radius: 6px;
  padding: 10px;
  margin: 0 20px 0 0;
}

.progress-tx {
  display: flex;
  margin-bottom: 2px;
  padding: 3px 0;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.c35 {
  color: #353535;
}

.bold {
  font-weight: bolder;
}

page {
  background: #efeff4;
  font-size: 12px;
}

.voc-header {
  background: #fff;
  margin: 10px;
  padding: 10px;
  border-radius: 6px;
  position: relative;
}

.voc-header .voc-item {
  padding: 2px 0;
}

.voc-header .voc-item label {
  padding: 0 2px 0 0;
}

.voc-header .voc-item text {
  color: #333;
  vertical-align: top;
}

.voc-item-con {
  background: #fff;
  margin: 10px;
  padding: 10px;
  border-radius: 6px;
  position: relative;
}

.voc-item-process {
  margin-bottom: 0;
}

.voc-item-con .voc-item-t {
  padding: 0px 0 5px 10px;
  position: relative;
  font-weight: bold;
}

.voc-item-con .voc-item-t:before {
  content: "";
  background: #1292dd;
  display: inline-block;
  height: 12px;
  width: 5px;
  position: absolute;
  left: 0px;
  top: 3px;
}

.voc-item-con view {
  padding: 3px 0;
}

.voc-item-con view textarea {
  width: auto;
  height: 80px;
  font-size: 12px;
  padding: 3px 5px;
  border-radius: 3px;
  margin-top: 2px;
  border: 1px solid #ddd;
}

.voc-item-con view label {
  padding: 0 5px 0 0;
}

.voc-pop .voc-item-con view label {
}

.voc-item-process view {
  display: flex;
}

.voc-item-process view label {
  width: 101px;
  display: inline-block;
}

.form-item-con {
  display: flex;
}

.form-item-con>label {
  width: 90px;
  display: inline-block;
}

.form-item-con>select {
  width: 100%;
  border: 1px solid #ccc;
  border-radius: 3px;
  padding: 3px 5px;
  font-size: 14px;
}

.form-item-con input {
  float: left;
  width: 100%;
  border: 1px solid #ccc;
  border-radius: 3px;
  padding: 2px 5px;
}

.order-list {
  padding: 16px;
  background: #fff;
}

.order-list-title {
  font-size: 14px;
  border-bottom: 1px solid #e5e5e5;
  margin-bottom: 10px;
}

.order-list ul li {
  border-bottom: 1px dashed #e5e5e5;
  padding: 15px 0;
}

.order-list-item {
  color: #333;
  white-space: nowrap;
  word-break: normal;
  overflow: hidden;
  text-overflow: ellipsis;
}

.order-list-account {
  color: #1292dd;
}

.add-voc {
  margin: 10px 0;
}

.btn-groups-voc {
  display: flex;
  margin: 0 10px 30px 10px;
}

.btn-groups-voc .add-voc {
  flex: 1;
  margin-top: 0px;
  margin: 0;
}

.btn-groups-voc .save-voc {
  margin-right: 10px;
}

.add-voc i {
  font-style: normal;
  font-size: 16px;
  display: inline-block;
  margin-right: 5px;
}

.order-list-btn {
  text-align: right;
  margin-top: 10px;
}

.item_button {
  width: 50px;
  height: 20px;
  display: inline-flex;
  color: #2180ff;
  font-size: 10px;
  background: #fff;
  justify-content: center;
  align-items: center;
  margin-left: 5px;
  border: 1px solid #2180ff;
}

.order-list-btn text {
  width: 50px;
  height: 20px;
  display: inline-flex;
  color: #2180ff;
  font-size: 10px;
  background: #fff;
  justify-content: center;
  align-items: center;
  border: 1px solid #2180ff;
  margin-left: 5px;
}

.order-list-btn text:hover {
  background: #1087cc;
}

.voc-pop .order-list-btn {
  text-align: center;
  margin-bottom: 30px;
  margin-top: 0px;
}

.voc-pop .order-list-btn text {
  margin: 0 10px;
  padding: 0px 30px;
  height: 30px;
  line-height: 30px;
  font-size: 14px;
}

.voc-pop {
  display: none;
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: #fff;
  overflow-y: auto;
}

.form-pop-item {
  padding: 0 16px;
  margin-top: 8px;
}

.form-pop-item label {
  display: block;
  margin-bottom: 5px;
  font-size: 14px;
  color: #333;
}

.form-pop-item input {
  padding: 6px 5px;
  border-radius: 3px;
  border: 1px solid #ccc;
  width: 100%;
  margin-bottom: 10px;
  font-size: 14px;
  color: #333;
}

.form-pop-edit-item label {
  display: inline-block;
}

.form-pop-edit-item {
  padding: 10px 16px;
}

.form-pop-edit-item select {
  border: 1px solid #ccc;
  border-radius: 3px;
  padding: 3px 5px;
}

.voc-pop .voc-item-con {
  margin-bottom: 0px;
}

.form-pop-item textarea {
  width: 100%;
  height: 80px;
  font-size: 14px;
  padding: 3px 5px;
  border-radius: 3px;
  border: 1px solid #ccc;
}

.voc-item-create>view {
  margin: 0px;
  padding: 0;
}

.voc-item-create>view label {
  display: block;
  position: relative;
  margin-bottom: 5px;
  width: 100%;
}

.voc-item-create>view text, .item_sub_button {
  display: inline-block;
  background: #1292dd;
  color: #fff;
  font-size: 12px;
  padding: 2px 10px;
  margin: 0 0 0 5px;
  border-radius: 5px;
  cursor: pointer;
}

.voc-item-create>view text:hover, .item_sub_button:hover {
  background: #1087cc;
}

.voc-item-create>view text:before {
  display: none;
}

.histroyMore {
  margin-top: 20px;
  color: #1292dd;
  font-size: 14px;
  cursor: pointer;
}

.voc-item-create .histroyMore-ul {
  margin-left: 3px;
  margin-top: 10px;
}

.voc-item-create .histroyMore-ul .histroyMore-li {
  border-left: 1px solid #e5e5e5;
  border-bottom: none;
  padding: 0;
  position: relative;
}

.voc-item-create .histroyMore-ul .histroyMore-li text {
  border-bottom: none;
  padding: 0 0 15px 10px;
  color: #333;
}

.voc-item-create .histroyMore-ul .histroyMore-li text:before {
  width: 8px;
  height: 8px;
  border-radius: 50%;
  left: -4px;
  top: 5px;
}

.histroyMore-p {
  padding: 0;
  margin: 0;
  color: #666;
  font-size: 12px;
  display: block;
}

.add-voc-button {
  border: 1px solid #2180ff;
  width: 100%;
  padding: 10px 0;
  margin-left: 0px;
}

.send-content {
  margin-bottom: 10px;
}

.item_sub_button {
  display: block;
  margin: 0 10px 30px 10px;
  font-size: 14px;
  line-height: normal;
  padding: 7px 0px;
}

.voc-item-con input {
  padding: 3px 5px;
  border-radius: 3px;
  margin-top: 2px;
  border: 1px solid #ccc;
}

.pickerFormItem {
  display: block;
}

.pickerFormItem .weui-input {
  width: auto;
  font-size: 12px;
  padding: 3px 5px;
  display: block;
  border-radius: 3px;
  margin-top: 2px;
  border: 1px solid #ddd;
  min-height: 1.4rem;
}

.pickerFormItem .weui-input image {
  float: right;
}
