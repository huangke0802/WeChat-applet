/* pages/error/error.wxss */
page{
  height: 100%
}
.v_logo{
  width: 100%;
  display:flex;
  flex-direction: row;
  justify-content:center;

}
.logo{
  width: 120px;
  height: 108px;
  padding-bottom: 40px;
 
}
.activity{
  width: 100%;
  height: 100%;
}
.login_container{
  padding-top: 30px;
}
.login_view{
  display:flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: flex-start;
  width: 75%;
  border: 1px solid #2E92F2;
  border-radius: 25px;
  line-height: 45px;
  height: 45px;
  margin: 0 auto;
  margin-bottom: 20px;
  padding-left: 20px;
  padding-right: 20px;
}
.login_lable{
  font-size: 12px;
  width: 25%;
}
.login_lableSel{
  font-size: 12px;
  width: 200px;
  color: #808080;
}
.login_text{
  height: 45px;
  font-size: 12px;
}
.login_button{
  width: 200px;
  background: #2E92F2;
  color: #fff;
  border-radius: 25px;
}
.title{
  width: 100%;
  display:flex;
  flex-direction: row;
  justify-content:center;
  font-size:18px;
  font-weight:bold;
  color: #272727;
}
.con{
   width: 100%;
  font-weight:normal;
  color: #9b9b9b;
  font-size:14px;
  padding-top: 20px;
  margin-left: 30px;
  margin-right: 30px
}